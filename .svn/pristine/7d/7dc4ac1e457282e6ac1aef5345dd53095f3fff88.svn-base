@charset "UTF-8";

body{
	min-width : 1500px;
	width: 100%;
	height: 2000px;
	background: #FFF0F0;
}
header{
	text-align: right;
	width : auto;
	height: 40px;
	font-size: 13px;
	padding : 1px;
	margin-bottom : 4px;
}
header li{
	float: right;
	margin-left: 10px;
	text-decoration: none;
	list-style: none;
	white-space: pre-wrap;
}
header a{
	text-decoration: none;
	color: #D3B88F;
	
}
nav{
	clear : both;
	margin : 10px;
	padding-left: 100px;
	font-size: 20px;
	text-decoration: none;
}
nav li{
	float: left;
	margin-left: 10px;
	text-decoration: none;
	list-style: none;
	white-space: pre-wrap;
}
nav a {
	text-decoration: none;
	color: black;
}
#main{
	width : 100%;
	padding-left: 100px;
	
}
.container{
	width : 100%;
	
}
#article1{
	width : 100%;
	height: 100%;
}
iframe{
	border-radius: 40px;
	width : 100%;
	height: 400px;
	border: none;
}
#leftboard{
	float : left;
	width : 38%;
	height: 300px;
	border: 2px solid #CC6666;
	margin : 13px;
	margin-left: 13%;
}
#rightboard{
	width : 38%;
	float : left;
	height: 300px;
	border: 2px solid #CC6666;
	margin : 13px;
	margin-left : 21px;
}
#funding{
	width : 78%;
	clear : both;
	margin : 13px;
	height: 300px;
	border: 2px solid #CC6666;
	margin-left: 13%;
	background: #FFF0F0;

}
#nanum{
	width : 78%;
	clear : both;
	margin : 13px;
	height: 300px;
	border: 2px solid #CC6666;
	margin-left: 13%;
	background: #FFF0F0;
}
#free{
	width : 100%;
	height: 100%;
	border-collapse: collapse;
	background: #FFF0F0;
}
#best{
	width : 100%;
	height: 100%;
	border-collapse: collapse;
	background: #FFF0F0;
}
#free td{
	padding-left: 10px;
	
}
#best td{
	padding-left: 10px;
}
hr { 
  display: block;
  margin-top: 0.5em;
  margin-bottom: 0.5em;
  margin-left: auto;
  margin-right: auto;
  border-style: inset;
  border-width: 1px;
  border-color:#7E4218;
} 
	
#spanhit{
	position: relative;
	float: right;
}


footer{
	clear : both;
	height: 40px;
	background: #EB9678;
	border-radius : 30px;
	text-align: center;
	font : 19px;
	padding-top: 10px;
	color : white;
	
}
.repl{
	background: #D2E1FF;
	border : 2px dotted black;
	
}

