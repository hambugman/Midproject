package vlog.vo;

public class FundVO {

	
	//펀딩게시판 테이블
	private int fun_no;		//펀딩번호p.k
	private String orp_no;		//보육원구성원번호p.k
	private String fun_name;	//번딩명
	private int fun_goal;	//펀딩목표금액
	private String fun_due;		//펀딩목표일
	private String fun_date;	//펀딩작성일
	private String fun_cont;	//펀딩내용
	
	
	
	public int getFun_no() {
		return fun_no;
	}
	public void setFun_no(int fun_no) {
		this.fun_no = fun_no;
	}
	public String getOrp_no() {
		return orp_no;
	}
	public void setOrp_no(String orp_no) {
		this.orp_no = orp_no;
	}
	public String getFun_name() {
		return fun_name;
	}
	public void setFun_name(String fun_name) {
		this.fun_name = fun_name;
	}
	public int getFun_goal() {
		return fun_goal;
	}
	public void setFun_goal(int fun_goal) {
		this.fun_goal = fun_goal;
	}
	public String getFun_due() {
		return fun_due;
	}
	public void setFun_due(String fun_due) {
		this.fun_due = fun_due;
	}
	public String getFun_date() {
		return fun_date;
	}
	public void setFun_date(String fun_date) {
		this.fun_date = fun_date;
	}
	public String getFun_cont() {
		return fun_cont;
	}
	public void setFun_cont(String fun_cont) {
		this.fun_cont = fun_cont;
	}
	
	
	

	
}
